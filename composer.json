{
    "name": "morningtrain/economic",
    "description": "PHP SDK for e-conomic",
    "keywords": [
        "morningtrain",
        "e-conomic"
    ],
    "homepage": "https://github.com/morning-train/economic",
    "license": "MIT",
    "authors": [
        {
            "name": "Martin Schadegg Brønniche",
            "email": "ms@morningtrain.dk",
            "role": "Developer"
        },
        {
            "name": "Simon Jønsson",
            "email": "sj@morningtrain.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.11"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.6",
        "pestphp/pest": "^2.15",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Morningtrain\\Economic\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Morningtrain\\Economic\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
